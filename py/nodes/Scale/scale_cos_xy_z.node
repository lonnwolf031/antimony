import fab

title('Scale (cosine, XY, Z)')

input('x0', float, 0)
input('y0', float, 0)
input('z0', float, 0)
input('z1', float, 1)
input('amplitude', float, 1)
input('offset', float, 0)
input('theta0', float, -45)
input('theta1', float, 45)

input('shape',fab.types.Shape)
output('transform', fab.shapes.scale_cos_xy_z(shape, x0, y0, z0, z1,
                                   amplitude, offset, theta0, theta1))

