import fab

title('Set origin (XY)')

input('a',fab.types.Shape)

input('x0', float, 0)
input('y0', float, 0)
input('x1', float, 1)
input('y1', float, 1)

output('out', fab.shapes.origin_xy(a, x0, y0, x1, y1))

# UI
sb.ui.wireframe([(x0, y0, 0), (x1, y1, 0)], color=sb.color.green)
sb.ui.point(x0, y0, color=sb.color.green)
sb.ui.point(x1, y1, color=sb.color.green)

